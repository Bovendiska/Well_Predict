<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.CapstoneBangkit" parent="Theme.Material3.DayNight">
        <!-- Customize your light theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimaryDark">@color/teal</item>
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="TextContent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/poppins_regular</item>
    </style>

    <style name="TextContent.Bold">
        <item name="fontFamily">@font/poppins_bold</item>

    </style>

    <style name="TextContent.Medium">
        <item name="fontFamily">@font/poppins_medium</item>
    </style>

    <style name="TextContent.SemiBold">
        <item name="fontFamily">@font/poppins_semibold</item>
    </style>

    <style name="Theme.CapstoneBangkit" parent="Base.Theme.CapstoneBangkit" />
</resources>